<h1>Stores</h1>

<% @stores.each do |store| %>
    <p><%= store.name %></p>
    <%= link_to 'Details', admin_store_path(store) %>
    <%= link_to 'Edit', edit_admin_store_path(store) %>
    <%= link_to 'Delete', admin_store_path(store), method: :delete%>
    <%= link_to 'Products', admin_store_product_stores_path(store) %>
<% end %>

<%= link_to 'Add store', new_admin_store_path %>
